        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="github.rutvijshah.apps.simpletodo.app.TodoMain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="github.rutvijshah.apps.simpletodo.app.edit_todo_item"
            android:label="@string/title_activity_edit_todo_item" >
        </activity>
    </application>

</manifest>
